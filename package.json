{
  "name": "pursuit-parking-log-app",
  "version": "1.0.0",
  "description": "Trello like app that allow users creating parking lots(lots) and adding notes to it",
  "main": "./bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "nodemon ./bin/www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dbm:create": "db-migrate create --migrations-dir ./db/migrations --config ./db/database.json",
    "dbm:up": "db-migrate up --migrations-dir ./db/migrations --config ./db/database.json",
    "dbm:down": "db-migrate down --migrations-dir ./db/migrations --config ./db/database.json",
    "dbm:reset": "db-migrate reset --migrations-dir ./db/migrations --config ./db/database.json",
    "heroku-prebuild": "npm run dbm:up"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alejo4373/Pursuit-Parking-Lot-App.git"
  },
  "keywords": [
    "trello",
    "lots",
    "columns",
    "post-it",
    "notes",
    "sticky",
    "website",
    "parking-lot"
  ],
  "author": "@alejo4373",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alejo4373/Pursuit-Parking-Lot-App/issues"
  },
  "homepage": "https://github.com/alejo4373/Pursuit-Parking-Lot-App#readme",
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "nodemon": "^2.0.3"
  },
  "dependencies": {
    "autoprefixer": "^9.7.6",
    "cookie-parser": "~1.4.4",
    "db-migrate": "^0.11.9",
    "db-migrate-pg": "^1.2.1",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "node-sass-middleware": "^0.11.0",
    "pg-promise": "^10.5.0",
    "postcss-middleware": "^1.1.4",
    "pug": "^2.0.4"
  }
}
